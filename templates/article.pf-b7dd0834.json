        "subtitle_TkCPwg": {
          "type": "subtitle",
          "settings": {
            "subtitle": "Über Mineralien, Schadstoffe, Fremdkörper und mehr...",
            "subtitle_tag": "h2",
            "subtitle_size": "h2",
            "font_weight": "100",
            "color": "#003e5f",
