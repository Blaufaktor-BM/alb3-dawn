        "heading_xiUXmb": {
          "type": "heading",
          "settings": {
            "heading": "<p>Alb Filter eco: Unser Beitrag zum Umweltschutz<\/p>",
            "remove_spacing": false,
            "color": "#003e5f",
            "heading_tag": "h2",
