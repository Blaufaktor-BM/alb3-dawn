        "heading_DifPch": {
          "type": "heading",
          "settings": {
            "heading": "<p>Unterschied Feingewinde und Grobgewinde <\/p><p><\/p>",
            "remove_spacing": false,
            "color": "#003e5f",
            "heading_tag": "h3",
            "heading_size": "h2",
            "font_weight": "700",
