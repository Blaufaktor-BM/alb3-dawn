      "settings": {
        "background_color": "#fafafa",
        "enable_border": false,
        "image": "shopify://shop_images/albteamfotolena--edited-1666259799650.jpg",
        "image_width": 150,
        "heading": "Noch Fragen zum Produkt?",
        "heading_tag": "h2",
