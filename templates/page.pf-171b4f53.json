    "multi_icon_text_zKCUpC": {
      "type": "multi-icon_text",
      "blocks": {
        "item_CLkx3F": {
          "type": "item",
          "settings": {
            "icon": "shopify:\/\/shop_images\/pf-f09c5a35--albfiltericonsetparameters01taste.svg",
            "heading": "Entnahme von schlechtem Geschmack und Gerüchen",
            "text": "<p>Hochwertige Alb Filter Kokosnuss Blockaktivkohle adsorbiert lästige Gerüche (Chlor, CKW’s, FCKW’s uvm.) und Geschmacksbeeinträchtigungen im Trinkwasser.<\/p>"
