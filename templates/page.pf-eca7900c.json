Promise((resolve) => {\n        \/\/ Wenn die Operation abgeschlossen ist, wird das Promise aufgelöst\n        resolve(rc);\n    });\n}\n\nasync function carouselCreation() {\n    \/\/ Warten, bis das Objekt erstellt wurde\n    const createdCarousel = await createReviewCarousel();\n    waitAndRemoveLowRatings();\n}\n\nfunction waitForWidget(selector) {\n    return new Promise((resolve) => {\n        const observer = new MutationObserver((mutationsList, observer) => {\n            for (let mutation of mutationsList) {\n                if (mutation.type === \"childList\") {\n                    const widget = document.querySelector(selector);\n                    if (widget) {\n                        observer.disconnect(); \/\/ Stop observing\n                        resolve(widget); \/\/ Resolve the promise with the widget\n                        break;\n                    }\n                }\n            }\n        });\n\n        \/\/ Start observing the document body for child node additions\n        observer.observe(document.body, { childList: true, subtree: true });\n    });\n}\n\nasync function waitAndRemoveLowRatings() {\n    const widgetSelector = \".R-ReviewsList__item\";\n    const widget = await waitForWidget(widgetSelector);\n\n    \/\/ Widget is now available\n    \/\/console.log(\"Widget found:\", widget);\n\n    var reviewContainers = document.querySelectorAll(\".R-ReviewsList__item\");\n    \/\/ Loop through each review container\n    reviewContainers.forEach(function (container) {\n        var ratingStars = container.querySelector(\".R-RatingStars__stars\");\n        if (ratingStars) {\n            var filledStars = ratingStars.querySelectorAll(\".stars__icon--0\").length;\n            if (filledStars > 1) {\n                container.style.display = \"none\";\n            }\n        }\n    });\n}\n\ncarouselCreation();\n\n<\/script>",
