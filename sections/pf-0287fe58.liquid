<script>
	{% assign prids = "wasserfilter-wasserhahn-duo-active,wasserfilter-wasserhahn-duo-active-plus,wasserfilter-wasserhahn-duo-nano" | split: ','%}
	var flowbars = ["70","50","90"];
	var flowdetail = ["Hoch: 9 l/min", "Mittel: 6 l/min", "Hoch: 11 l/min"];

	function move(id, percent) {
		var elem = document.getElementById(id);
		var width = 1;
		var id = setInterval(frame, 10);
		function frame() {
		  if (width >= percent) {
			clearInterval(id);
		  } else {
			width++;
			elem.style.width = width + "%";
		  }
		}
	}
	
	var kartuschenTabelle = new trulyResponsiveTable('kartuschenTabelle');
	var tmp_price, i = 0;
	var delimiter = ','; // Komma als trenner des Preises
	 
	 {% for item in prids %}
		item = '{{ item }}';
		tmp_price = '{{ all_products[item].price | money_without_currency }}';
		tmp_price = tmp_price.split(delimiter);
		document.getElementById('cartridge-' + i + '-price').getElementsByClassName('integer-part')[0].innerHTML = tmp_price[0];
		document.getElementById('cartridge-' + i + '-price').getElementsByClassName('decimal-part')[0].innerHTML = tmp_price[1];
		// Flowbar
		move("flowbar-"+i, flowbars[i]);
		document.getElementById('flowdetail-' + i).innerHTML = flowdetail[i];
		i++;
	{% endfor %}
	
	
	
	
</script>


