        "heading_4iVC6M": {
          "type": "heading",
          "settings": {
            "heading": "<p>Wasserfilter und Nachhaltigkeit: Wie passt das zusammen?<\/p>",
            "heading_tag": "h2",
            "heading_size": "h3",
            "heading_weight": "700",
            "second_font": false,
