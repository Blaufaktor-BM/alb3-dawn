<script>
  let modulocounter = 0;
  let intervalId;


const images = [
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Active_db204cad-1f10-4797-8d79-7490c67da908.svg?v=1716797406',
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Active_Protect_a303b5ba-a627-433b-887c-4550ed922e5a.svg?v=1716797407',
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Fusion_2522c840-d91e-41e6-996e-91550ae69f87.svg?v=1716797406',
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Fusion_Protect_b2cb274f-57b3-4787-9e24-b3a4623d2f9d.svg?v=1716797406',
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Nano_Protect_6b7f46b7-685e-46ae-b28d-e92e382373e4.svg?v=1716797406',
  'https://cdn.shopify.com/s/files/1/0278/5483/1715/files/Mobil_Nano_8c328850-2903-416c-9c05-bbc37eb31c4f.svg?v=1716797407'
];

const imgswitcher = document.getElementById('img-switcher');
const dotsContainer = document.getElementById('dots-container');

// Create dots
images.forEach((image, index) => {
  const dot = document.createElement('span');
  dot.classList.add('dot');
  dot.style.height = '10px';
  dot.style.width = '10px';
  dot.style.margin = '0 5px';
  dot.style.display = 'inline-block';
  dot.style.borderRadius = '50%';
  dot.style.backgroundColor = index === 0 ? 'darkgray' : 'lightgray';
  dot.addEventListener('click', () => switchImage(index));
  dotsContainer.appendChild(dot);
});

function updateDots() {
  const dots = dotsContainer.getElementsByClassName('dot');
  for (let i = 0; i < dots.length; i++) {
    dots[i].style.backgroundColor = i === (modulocounter % images.length) ? 'darkgray' : 'lightgray';
  }
}

function switchImage(index) {
  modulocounter = index;
  imgswitcher.src = images[index];
  updateDots();
}

function startImageSwitch() {
  intervalId = setInterval(() => {
    switchImage(modulocounter % images.length);
    modulocounter++;
  }, 2000);
}

function stopImageSwitch() {
  clearInterval(intervalId);
}

let startX = 0;
let currentIndex = 0;

imgswitcher.addEventListener('mouseover', stopImageSwitch);
imgswitcher.addEventListener('mouseout', startImageSwitch);
imgswitcher.addEventListener('touchstart', (e) => {
  stopImageSwitch();
  startX = e.touches[0].clientX;
});

imgswitcher.addEventListener('touchend', (e) => {
  const touchX = e.changedTouches[0].clientX;
  const diffX = touchX - startX;

  if (diffX > 50) {
    currentIndex = (modulocounter - 1 + images.length) % images.length;
  } else if (diffX < -50) {
    currentIndex = (modulocounter + 1) % images.length;
  }

  switchImage(currentIndex);
  startImageSwitch();
});

// Start the image switch initially
startImageSwitch();
</script>

